{
  "patterns": [
    {
      "id": "empty_key",
      "name": "Empty Translation Keys",
      "description": "Keys that are empty strings or contain only quotes/brackets",
      "type": "key_pattern",
      "pattern": "^$",
      "testOn": "key",
      "action": "remove",
      "replacement": "",
      "category": "empty",
      "enabled": true,
      "examples": [
        "",
        "\"",
        "[",
        "]"
      ]
    },
    {
      "id": "bracket_only_value",
      "name": "Bracket-Only Values",
      "description": "Values that are just brackets, quotes, or similar characters",
      "type": "value_pattern",
      "pattern": "^[\\[\\]\"'`]+$",
      "testOn": "value",
      "action": "remove",
      "replacement": "",
      "category": "empty",
      "enabled": true,
      "examples": [
        "[",
        "]",
        "\"",
        "'",
        "```"
      ]
    },
    {
      "id": "placeholder_number",
      "name": "Placeholder Numbers",
      "description": "Keys like _1, _2, _100 used as placeholders",
      "type": "key_pattern",
      "pattern": "^_\\d+$",
      "testOn": "key",
      "action": "remove",
      "replacement": " ",
      "category": "placeholder",
      "enabled": true,
      "examples": [
        "_1",
        "_2",
        "_100"
      ]
    },
    {
      "id": "single_underscore",
      "name": "Single Underscore",
      "description": "Keys that are just a single underscore",
      "type": "key_pattern",
      "pattern": "^_$",
      "testOn": "key",
      "action": "remove",
      "replacement": "_",
      "category": "underscore",
      "enabled": true,
      "examples": [
        "_"
      ]
    },
    {
      "id": "only_numbers",
      "name": "Number-only Values",
      "description": "Values containing only numbers",
      "type": "value_pattern",
      "pattern": "^\\d+$",
      "testOn": "value",
      "action": "remove",
      "replacement": "self",
      "category": "number",
      "enabled": true,
      "examples": [
        "0",
        "1",
        "100",
        "2024"
      ]
    },
    {
      "id": "percentage_pattern",
      "name": "Percentage Values",
      "description": "Values that are percentages",
      "type": "value_pattern",
      "pattern": "^\\d+%$",
      "testOn": "value",
      "action": "remove",
      "replacement": "self",
      "category": "number",
      "enabled": true,
      "examples": [
        "100%",
        "50%",
        "0%"
      ]
    },
    {
      "id": "decimal_numbers",
      "name": "Decimal Numbers",
      "description": "Decimal number values",
      "type": "value_pattern",
      "pattern": "^\\d+\\.\\d+$",
      "testOn": "value",
      "action": "remove",
      "replacement": "self",
      "category": "number",
      "enabled": true,
      "examples": [
        "1.0",
        "3.14",
        "99.99"
      ]
    },
    {
      "id": "simple_symbols",
      "name": "Simple Symbol Values",
      "description": "Values that are just basic symbols",
      "type": "value_pattern",
      "pattern": "^[\\*\\+\\-=<>]{1,3}$",
      "testOn": "value",
      "action": "remove",
      "replacement": "self",
      "category": "symbol",
      "enabled": true,
      "examples": [
        "*",
        "+",
        "-",
        "=",
        "<",
        ">",
        "**"
      ]
    },
    {
      "id": "whitespace_only",
      "name": "Whitespace Only",
      "description": "Values containing only whitespace",
      "type": "value_pattern",
      "pattern": "^\\s+$",
      "testOn": "value",
      "action": "remove",
      "replacement": " ",
      "category": "whitespace",
      "enabled": true,
      "examples": [
        "  ",
        "   "
      ]
    },
    {
      "id": "untitled_key",
      "name": "Untitled Keys",
      "description": "Keys that are 'untitled' which often create empty nested objects",
      "type": "key_pattern",
      "pattern": "^untitled$",
      "testOn": "key",
      "action": "remove",
      "replacement": "",
      "category": "empty",
      "enabled": true,
      "examples": [
        "untitled"
      ]
    },
    {
      "id": "nested_empty_objects",
      "name": "Nested Empty Objects",
      "description": "Keys that create empty nested object structures with empty values",
      "type": "structure_pattern",
      "pattern": ".*\\.untitled$",
      "testOn": "key",
      "action": "remove",
      "replacement": "",
      "category": "empty",
      "enabled": true,
      "examples": [
        "component.untitled",
        "dialog.untitled"
      ]
    },
    {
      "id": "execution_error_value",
      "name": "Technical Error Messages",
      "description": "Values containing technical execution errors and stack trace messages, not user-facing error labels",
      "type": "value_pattern",
      "pattern": "^(Execution error|TypeError|ReferenceError|Stack trace|Uncaught|at Object\\.|at Function\\.|Error: |Exception: |Traceback|\\w+Exception$).*$",
      "testOn": "value",
      "action": "remove",
      "replacement": "",
      "category": "error",
      "enabled": true,
      "examples": [
        "Execution error",
        "TypeError: Cannot read",
        "ReferenceError: x is not defined",
        "Uncaught Error",
        "at Object.function",
        "Exception: Invalid input"
      ]
    }
  ],
